{
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "intelephense.telemetry.enabled": false,
    "redhat.telemetry.enabled": false,
    "telemetry.telemetryLevel": "off",
    "editor.minimap.enabled": false,
    "editor.snippetSuggestions": "top",
    "editor.renderControlCharacters": false,
    "editor.renderWhitespace": "all",
    "editor.copyWithSyntaxHighlighting": false,
    "editor.wordSeparators": "`~!#%^&*()-=+[{]}\\|;:'\",.<>/?",
    "editor.wordWrapColumn": 120,
    "editor.rulers": [ 120 ],
    "editor.fontFamily": "JetBrains Mono",
    "editor.wordWrap": "on",
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "terminal.integrated.copyOnSelection": true,
    "workbench.sideBar.location": "left",
    "search.searchOnTypeDebouncePeriod": 400,
    "search.showLineNumbers": true,
    "files.trimTrailingWhitespace": true,
    "window.menuBarVisibility": "toggle",
    "extensions.ignoreRecommendations": false,
    "terminal.integrated.scrollback": 2200,
    "terminal.integrated.rightClickBehavior": "paste",
    "diffEditor.renderSideBySide": true,
    "[makefile]": {
        "editor.insertSpaces": false,
        "editor.detectIndentation": false
    },
    "files.insertFinalNewline": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "workbench.enableExperiments": false,
    "problems.showCurrentInStatus": true,
    "search.quickOpen.history.filterSortOrder": "recency",
    "git.enableSmartCommit": true,
    "git.smartCommitChanges": "tracked",
    "git.suggestSmartCommit": false,
    "explorer.autoReveal": false,
    "cSpell.enableFiletypes": [
        "!java",
        "!rust",
        "!scala",
        "!scss",
        "haml",
        "ruby"
    ],
    "cSpell.language": "en,ru",
    "cSpell.userWords": [
        "Covid",
        "cpath",
        "Emias",
        "heightmap",
        "mutators",
        "noinput",
        "notheme"
    ],
    "githd.logView.displayExpressStatus": false,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.panel.defaultLocation": "right",
    "[php]": {
        "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
    },
    "files.associations": {
        "*.blade.php": "html",
        "*.tpl": "html"
    },
    "phpSniffer.autoDetect": true,
    "phpSniffer.standard": "PSR12",
    "phpSniffer.run": "onType",
    "workbench.colorTheme": "Gruvbox Dark Medium",
    "toggler.toggles": [
        [
            "private",
            "protected",
            "public"
        ],
        [
            "public static function",
            "protected static function",
            "private static function",
        ]
    ],
    "files.defaultLanguage": "${activeEditorLanguage}",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "python.pythonPath": "/usr/bin/python3",
    "security.workspace.trust.enabled": false,
    "files.autoSaveDelay": 1500,
    "workbench.activityBar.visible": false,
    "json.maxItemsComputed": 7000,
    "terminal.integrated.defaultProfile.linux": "/bin/bash (migrated)",
    "terminal.integrated.profiles.linux": {
        "/bin/bash (migrated)": {
            "path": "/bin/bash",
            "args": []
        }
    },
    "files.autoSave": "onFocusChange",
    "update.mode": "manual",
    "vva.trustedDomain": true,
    "terminal.integrated.tabs.enabled": false,
}
